{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Courses",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "credits": {
      "type": "integer",
      "required": true
    },
    "enrollments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::enrollment.enrollment",
      "mappedBy": "courseId"
    },
    "exams": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exam.exam",
      "mappedBy": "courseId"
    },
    "examResults": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exam-result.exam-result",
      "mappedBy": "course"
    },
    "subject": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subject.subject",
      "inversedBy": "courses"
    },
    "code": {
      "type": "string"
    },
    "theoryCount": {
      "type": "integer"
    },
    "praticeCount": {
      "type": "integer"
    }
  }
