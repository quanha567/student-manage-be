{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Courses",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "courseName": {
      "type": "string",
      "required": true
    },
    "credits": {
      "type": "integer",
      "required": true
    },
    "enrollments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::enrollment.enrollment",
      "mappedBy": "courseId"
    },
    "exams": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exam.exam",
      "mappedBy": "courseId"
    },
    "courseId": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid"
    }
  }
}
