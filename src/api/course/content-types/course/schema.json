{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Courses",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "credits": {
      "type": "integer",
      "required": true
    },
    "exams": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exam.exam",
      "mappedBy": "courseId"
    },
    "examResults": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exam-result.exam-result",
      "mappedBy": "course"
    },
    "subject": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subject.subject",
      "inversedBy": "courses"
    },
    "code": {
      "type": "string"
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "endDate": {
      "type": "date"
    },
    "courseType": {
      "type": "enumeration",
      "enum": [
        "REQUIRED",
        "OPTIONAL"
      ]
    },
    "semester": {
      "type": "string"
    },
    "labHours": {
      "type": "integer"
    },
    "lectureHours": {
      "type": "integer"
    },
    "description": {
      "type": "richtext"
    },
    "classes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::class.class",
      "mappedBy": "course"
    },
    "sections": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::section.section",
      "mappedBy": "course"
    }
  }
}
