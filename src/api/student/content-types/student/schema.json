{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Students",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "dateOfBirth": {
      "type": "date"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "MALE",
        "FEMALE",
        "OTHER"
      ]
    },
    "address": {
      "type": "text"
    },
    "phoneNumber": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "classId": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::class.class",
      "mappedBy": "students"
    },
    "enrollments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::enrollment.enrollment",
      "mappedBy": "studentId"
    },
    "examResults": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exam-result.exam-result",
      "mappedBy": "studentId"
    },
    "studentId": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid"
    },
    "userId": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "studentId"
    }
  }
}
